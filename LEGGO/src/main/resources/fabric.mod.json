{
  "schemaVersion": 1,
  "id": "ardis-the-legend",
  "version": "${version}",

  "name": "Ardis The Legend",
  "description": "Ardis The Legend is the product when a meme mod gets serious... The time has come to reveal the secrets behind the legend Ardis.",
  "authors": [
    "nieli",
    "Ardisinius/Ardis"
  ],
  "contact": {
    "sources": "https://github.com/niieli/ardis-the-legend-fabricmcmod",
    "issues": "https://github.com/niieli/ArdisTheLegend/issues"
  },

  "license": "MIT",
  "icon": "assets/ardis-the-legend/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.nieli.ardis.ArdisTheLegendMod",
      "net.nieli.ardis.registry.EntityRegistry",
      "net.nieli.ardis.ArdisOreGeneration"
    ],
    "client": [
      "net.nieli.ardis.client.EntityClient",
      "net.nieli.ardis.client.ArdisTheLegendClient"
    ],
    "terrablender": [
      "net.nieli.ardis.worldgen.biomes.BiomeRegistry"
    ]
  },

  "mixins": [
    "ardis.mixins.json"
  ],

  "depends": {
    "geckolib3": ">=3.0.23",
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
